{
	"name": "EK",
	"scopeName": "source.ek",
	"patterns": [
	  {
		"name": "comment.line.double-slash.ek",
		"match": "//.*",
		"captures": {
		  "0": { "name": "comment.line.ek" }
		}
	  },
	  {
		"name": "comment.block.ek",
		"begin": "/\\*",
		"end": "\\*/",
		"captures": {
		  "0": { "name": "comment.block.ek" }
		}
	  },
	  {
		"name": "keyword.control.ek",
		"match": "\\b(atom|struct|type|fn|lazy|precedence|import|is)\\b"
	  },
	  {
		"name": "storage.type.ek",
		"match": "\\b(bool|int|true|false|string|void|any|never|float)\\b"
	  }
	],
	"repository": {
	  "comment": {
		"patterns": [
		  {
			"name": "comment.line.ek",
			"match": "//.*"
		  },
		  {
			"name": "comment.block.ek",
			"begin": "/\\*",
			"end": "\\*/"
		  }
		]
	  },
	  "keywords": {
		"patterns": [
		  {
			"name": "keyword.control.ek",
			"match": "\\b(atom|struct|type|fn|lazy|precedence|import|is)\\b"
		  }
		]
	  },
	  "types": {
		"patterns": [
		  {
			"name": "storage.type.ek",
			"match": "\\b(bool|int|true|false|string|void|any|never|float)\\b"
		  }
		]
	  }
	}
}
